{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.ts"
        }
    ],
    "crons": [
        {
            "path": "/incentive/cron",
            "schedule": "*/5 * * * *"
        }
    ]
}